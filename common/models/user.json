{
  "name": "user",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "user_type": {
      "type": "string",
      "required": true,
      "default": "buyer"
    },
    "items": {
      "type": "array",
      "required": false,
      "default": []
    },
    "phoneNumber": {
      "type": "number",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "password": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "orders": {
      "type": "hasMany",
      "model": "order",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "listing": {
      "type": "hasMany",
      "model": "listing",
      "foreignKey": "sellerId",
      "options": {
        "nestRemoting": false
      }
    }
  },
  "acls": [],
  "methods": {}
}
